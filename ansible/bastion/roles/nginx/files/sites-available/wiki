server {
    listen 443 ssl ;
    ssl_certificate /etc/letsencrypt/live/wiki.nova-ten.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/wiki.nova-ten.com/privkey.pem; # managed by Certbot

    server_name wiki.nova-ten.com; # managed by Certbot


    location / {
        proxy_pass http://10.69.46.162:80;
        include proxy_params;
    }
    
    location /dash-a/ {
        proxy_pass https://10.69.46.162:30443/;
        include proxy_params;
        proxy_ssl_verify off;
    }

}

server {
    if ($host = wiki.nova-ten.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80 ;
    server_name wiki.nova-ten.com;
    return 404; # managed by Certbot

}